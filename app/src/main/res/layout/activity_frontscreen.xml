<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".frontscreen"
    android:fitsSystemWindows="true">

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/ecg"
        android:layout_width="405dp"
        android:layout_height="178dp"
        android:alpha="1"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.517"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/ecg"
        app:lottie_repeatCount="0" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/buttonback"
        android:layout_width="236dp"
        android:layout_height="93dp"
        android:alpha="1"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.849"
        app:lottie_autoPlay="true"
        app:lottie_colorFilter="#A73737"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/redbutt2"
        app:lottie_repeatCount="0" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="294dp"
        android:layout_height="60dp"
        android:fontFamily="@font/comfortaa_light"
        android:gravity="center"
        android:text="WELCOME TO"
        android:textColor="#777777"
        android:textSize="36sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <TextView
        android:id="@+id/sigin"
        android:layout_width="294dp"
        android:layout_height="60dp"
        android:fontFamily="sans-serif-light"
        android:gravity="center"
        android:text="Already have an account? Sign in."
        android:textColor="#929292"
        android:textSize="14sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.956" />

    <TextView
        android:id="@+id/getstart"
        android:layout_width="205dp"
        android:layout_height="66dp"
        android:fontFamily="@font/comfortaa_light"
        android:gravity="center"
        android:text="Get Started"
        android:textColor="#F8ECC1C1"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.834" />

    <TextView
        android:id="@+id/appname"
        android:layout_width="332dp"
        android:layout_height="86dp"
        android:fontFamily="@font/comfortaa_light"
        android:gravity="center"
        android:text="CHANTIX"
        android:textColor="#F8B90C0C"
        android:textSize="50sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.196" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/heartani"
        android:layout_width="560dp"
        android:layout_height="473dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.542"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/heart" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/heartback"
        android:layout_width="494dp"
        android:layout_height="402dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.525"
        app:lottie_autoPlay="true"
        app:lottie_colorFilter="#3C3F41"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/heartback2"
        app:lottie_repeatCount="0" />

    <com.airbnb.lottie.LottieAnimationView
        android:id="@+id/heartback2"
        android:layout_width="494dp"
        android:layout_height="402dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.525"
        app:lottie_autoPlay="true"
        app:lottie_loop="true"
        app:lottie_rawRes="@raw/heartback2"
        app:lottie_repeatCount="0" />

</androidx.constraintlayout.widget.ConstraintLayout>